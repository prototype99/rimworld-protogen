<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>BionicProtogenTail</defName>
    <label>bionic protogen tail</label>
	<description>A bionic protogen tail. Great for the manipulation and movement of such a species.</description>
    <spawnThingOnRemoved>BionicProtogenTail</spawnThingOnRemoved>
    <addedPartProps>
      <betterThanNatural>true</betterThanNatural>
      <solid>true</solid>
      <partEfficiency>1.2</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
          <capMods>
            <li>
              <capacity>Manipulation</capacity>
              <offset>0.2</offset>
            </li>
			 <li>
               <capacity>Moving</capacity>
               <offset>0.2</offset>
            </li>
          </capMods>
      </li>
    </stages>
  </HediffDef>

    <HediffDef ParentName="AddedBodyPartBase">
    <defName>ClothTail</defName>
    <label>cloth tail</label>
	<description>A cloth tail. Not so good, but better than nothing.</description>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.6</partEfficiency>
    </addedPartProps>
  </HediffDef>
  
</Defs>