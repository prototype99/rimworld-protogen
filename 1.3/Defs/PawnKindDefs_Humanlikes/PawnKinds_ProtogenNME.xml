  <PawnKindDef Name="ProtogenCivBaseNME" ParentName="ProtogenBaseNME" Abstract="True">
    <combatPower>25</combatPower>
	<race>Alien_ProtogenNME</race>
	<initialWillRange>11~33</initialWillRange>
    <initialResistanceRange>15~43</initialResistanceRange>
    <itemQuality>Normal</itemQuality>
    <isFighter>false</isFighter>
    <gearHealthRange>0.2~2.0</gearHealthRange>
