    <weaponMoney>1400~2500</weaponMoney>
    <weaponTags>
      <li>Gun</li>
      <li>NonNeolithicOnlyMelee</li>
    </weaponTags>
    <techHediffsMoney>200~300</techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.51</techHediffsChance>
    <invNutrition>2.55</invNutrition>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>90</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>25~70</countRange>
